struct Developer{
    var name : string ;
    var job : string ;
}

function Im_Developer(name:string,job:string): Developer{
    var tmp : Developer ;
    tmp.name = name;
    tmp.job = job;
    return tmp ;
}



var Hasan : Developer;
Hasan = Im_Developer("Hasan Kashi","Golang,Pascal,Dlang Developer");

print(Hasan.name);
print(Hasan.job);