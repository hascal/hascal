# standard hascal physic library
function Density(m:float,v:float):float{
    return m / v;
}

function Pressure(f:float,a:float):float {
    return f / a ;
}

function Work(f:float,d:float):float{
    return f * d ;
}

function Work2(f:float,teta:float,d:float):float {
    return f * cos(teta) * d ;
}

function Kinetic_Energy(m:float,v:float):float{
    return m / 2 * (v * v);
}

function cm_to_m(i:float):float{
    return i / 100 ;
}

function m_to_cm(i:float):float {
    return i * 100;
}